<?xml version='1.0' encoding='utf-8'?>
<widget id="com.codesundar.sqlite" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Password Saver</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon platform="android" qualifier="ldpi" src="res/icon/android/ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="res/icon/android/mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="res/icon/android/hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="res/icon/android/xhdpi.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
